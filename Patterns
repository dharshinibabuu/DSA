import java.util.Scanner;
public class Main{
    public static void main(String[] args){
        Scanner scanner=new Scanner(System.in);
        int N=scanner.nextInt();
        for(int i=1;i<=N;i++){
            for(int j=1;j<=i;j++){
            System.out.print("*");
        }
        System.out.println();
        }
        for(int i=1;i<=N;i++){
            for(int j=1;j<=N-i;j++){
            System.out.print("*");
        }
        System.out.println();
        }
        
        scanner.close();
    }
}

9.N=4
   *
  ***
 *****
*******

import java.util.Scanner;
public class Main {
    public static void main(String[] args) {
        Scanner scanner = new Scanner(System.in);
        int N = scanner.nextInt();
        for (int i = 1; i <= N; i++) {
            for (int j = 1; j <= N - i; j++) {
                System.out.print(" ");
            }
            for (int j = 1; j <= 2 * i - 1; j++) {
                System.out.print("*");
            }
            System.out.println();
        }

        scanner.close();
    }
}

10.N=4
